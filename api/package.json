{
  "name": "iustino.com-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon express/index.js",
    "dev:all": "concurrently \"npm run strapi:build\" \"npm run strapi:develop\" \"npm run dev\"",
    "start": "node express/index.js",
    "strapi:build": "cd strapi && npm run build",
    "strapi:develop": "cd strapi && npm run develop",
    "strapi:start": "cd strapi && npm run start"
  },
  "dependencies": {
    "@strapi/provider-upload-aws-s3": "^4.25.11",
    "@strapi/strapi": "^4.25.11",
    "aws-sdk": "^2.1665.0",
    "better-sqlite3": "8.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-proxy-middleware": "^3.0.2"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "nodemon": "^2.0.22"
  }
}
